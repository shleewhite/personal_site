{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "eslint-plugin-html"
    ],
    "rules": {
         "indent": ["error", 2],
         "no-multiple-empty-lines": ["error"]
    }
}
